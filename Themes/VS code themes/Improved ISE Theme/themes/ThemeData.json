{
    "DarkColors": {
        "editor.foreground": "#d4d4d4",
        "editorIndentGuide.activeBackground": "#707070",
        "editorSuggestWidget.background": "#141414",
        "editorSuggestWidget.foreground": "#ffffff",
        "editorSuggestWidget.highlightForeground": "#ff9600",
        "editorSuggestWidget.selectedBackground": "#0057ad",
        "list.hoverBackground": "#094771",
        "list.hoverForeground": "#ffffff",
        "menu.background": "#252526",
        "menu.foreground": "#cccccc",
        "panelTitle.activeBorder": "#09ff00",
        "terminal.background": "#012456"
    },
    "LightColors": {
        "activityBar.background": "#f5f3f1",
        "activityBar.foreground": "#000000",
        "editorGroupHeader.tabsBackground": "#f5e9d9",
        "editorSuggestWidget.background": "#ffffff",
        "editorSuggestWidget.foreground": "#000000",
        "editorSuggestWidget.highlightForeground": "#ff9600",
        "list.hoverBackground": "#0074e8",
        "list.hoverForeground": "#ffffff",
        "panel.background": "#e1d5c5",
        "panelTitle.activeBorder": "#ff0000",
        "panelTitle.activeForeground": "#000000",
        "panelTitle.inactiveForeground": "#000000",
        "settings.numberInputBorder": "#CECECE",
        "settings.textInputBorder": "#CECECE",
        "sideBar.background": "#efefef",
        "sideBar.foreground": "#000000",
        "statusBar.background": "#999999",
        "statusBar.debuggingBackground": "#ff4500",
        "statusBar.noFolderBackground": "#999999",
        "tab.activeBackground": "#e0ebfc",
        "tab.activeForeground": "#000000",
        "tab.inactiveBackground": "#eeeeee",
        "tab.inactiveForeground": "#000000",
        "terminal.background": "#012456",
        "terminal.foreground": "#f5f5f5",
        "titleBar.activeBackground": "#dfeaf7",
        "titleBar.activeForeground": "#000000"
    },
    "tokenColors": [
        {
            "name": "Attributes",
            "scope": [
                "support.function.attribute",
                "punctuation.definition.tag"
            ],
            "DarkSettings": {
                "foreground": "#00bfff"
            },
            "LightSettings": {
                "foreground": "#00bfff"
            }
        },
        {
            "name": "Commands",
            "scope": [
                "support.function"
            ],
            "DarkSettings": {
                "foreground": "#0096ff"
            },
            "LightSettings": {
                "foreground": "#0000ff"
            }
        },
        {
            "name": "FunctionNameDefinitionAndParamValue",
            "scope": [
                "entity.name.function",
                "string.unquoted.argument",
                "keyword.control.switch-default"
            ],
            "DarkSettings": {
                "foreground": "#ff96ff"
            },
            "LightSettings": {
                "foreground": "#8a2be2"
            }
        },
        {
            "name": "ParamNameAndLoopLabel",
            "scope": [
                "variable.parameter",
                "entity.name.label"
            ],
            "DarkSettings": {
                "foreground": "#d4d4d4"
            },
            "LightSettings": {
                "foreground": "#00008b"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment"
            ],
            "DarkSettings": {
                "foreground": "#6A9955"
            },
            "LightSettings": {
                "foreground": "#006400"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.control",
                "keyword.declaration",
                "storage.modifier"
            ],
            "DarkSettings": {
                "foreground": "#b2b200"
            },
            "LightSettings": {
                "foreground": "#00008b"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "DarkSettings": {
                "foreground": "#dc64dc"
            },
            "LightSettings": {
                "foreground": "#800080"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "punctuation.definition.parameter",
                "keyword.operator",
                "punctuation.section.bracket",
                "string.quoted.double.xml",
                "string.quoted.single.xml"
            ],
            "DarkSettings": {
                "foreground": "#d4d4d4"
            },
            "LightSettings": {
                "foreground": "#696969"
            }
        },
        {
            "name": "WhiteBlack",
            "scope": [
                "text.xml",
                "variable.parameter.attribute",
                "keyword.other",
                "entity.name",
                "constant.character.escape",
                "variable.other.property",
                "punctuation.terminator.statement",
                "keyword.other.hashtable",
                "punctuation.section",
                "entity.name.variable",
                "entity.name.function.method",
                "punctuation.accessor.powershell",
                "variable.other.object.property"
            ],
            "DarkSettings": {
                "foreground": "#ffffff"
            },
            "LightSettings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string.quoted",
                "entity.name.tag.localname",
                "entity.name.tag.xml"
            ],
            "DarkSettings": {
                "foreground": "#dc8282"
            },
            "LightSettings": {
                "foreground": "#8b0000"
            }
        },
        {
            "name": "Types",
            "scope": [
                "keyword.type",
                "support.class",
                "storage.type",
                "punctuation.accessor.type",
                "entity.name.type.class",
                "entity.other.inherited-class"
            ],
            "DarkSettings": {
                "foreground": "#00afaf"
            },
            "LightSettings": {
                "foreground": "#006161"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "constant.language",
                "entity.other.attribute-name",
                "storage.modifier.scope",
                "keyword.type.drive"
            ],
            "DarkSettings": {
                "foreground": "#87ceeb"
            },
            "LightSettings": {
                "foreground": "#ff4500"
            }
        },
        {
            "name": "XMLcData",
            "scope": [
                "string.unquoted.cdata.xml"
            ],
            "DarkSettings": {
                "foreground": "#9e9e9e"
            },
            "LightSettings": {
                "foreground": "#808080"
            }
        }
    ]
}